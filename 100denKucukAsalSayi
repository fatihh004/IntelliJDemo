import java.util.Scanner;

public class asalSayiBulma {
    public static void main(String[] args) {
        int a = 100, b = 0 ;
        System.out.println("100'den küçük Asal Sayılar : ");
        while (a > b) {
            b++;
            if ((b%2 != 0 || b/2==1 ) && (b%3!= 0 || b /3 ==1 )&& (b %5!= 0 || b /5 ==1) && (b %7!= 0 || b /5 ==1) && b != 1) {
                System.out.print(b+",");
            }
        }

    }
}


